{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "removeComments": false,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": [
      "./src/types"
    ],
    "declaration": true,
    "declarationDir": "./types",
    "declarationMap": false,
    "rootDir": "./src/types",
    "sourceRoot": "./src/types",
    "outDir": "./dist",
    "baseUrl": "./src/types",
    "preserveConstEnums": true
  },
  "files": [
    "./src/types/game.ts"
  ],
  "include": [
    "src/types/*"
  ],
  "exclude": [
    "src/game"
  ]
}
